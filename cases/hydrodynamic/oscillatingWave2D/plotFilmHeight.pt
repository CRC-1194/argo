#Script to plot film height at center of domain for different resolutions

reset


#--------------------------------------------------------------------------------------------

set terminal pngcairo size 1080,680 enhanced font 'Verdana,10'




#STYLE
set style line 1 lc rgb '#ff8c69' pt 1 ps 1 lt 1 lw 2 # --- salmon
set style line 2 lc rgb '#5e9c36' pt 6 ps 1 lt 1 lw 2 # --- green
set style line 3 lc rgb '#1520A6' pt 6 ps 1 lt 1 lw 2 # --- azureblue
set style line 4 lc rgb '#ff00ff' pt 6 ps 1 lt 1 lw 2 # --- fuchsia



set style line 11 lc rgb '#808080' lt 1
set border 3 back ls 11
set tics nomirror

set style line 12 lc rgb '#808080' lt 0 lw 1
set grid back ls 12

#--------------------------------------------------------------------------------------------

set output 'OscillatingWaveFilmHeight.png'


set xlabel "Time [s]"
set ylabel "film deflection [m]"


set title "Film height for oscillating wave"

set grid

set key left top


plot "Resolution128/postProcessing/interfaceHeight1/0/height.dat" u 1:($2-0.0013) title "res 128" with lines  ls 1,\
"Resolution64/postProcessing/interfaceHeight1/0/height.dat" u 1:($2-0.0013) title "res 64" with lines ls 2,\
"Resolution32/postProcessing/interfaceHeight1/0/height.dat" u 1:($2-0.0013) title "res 32" with lines ls 3


reset
