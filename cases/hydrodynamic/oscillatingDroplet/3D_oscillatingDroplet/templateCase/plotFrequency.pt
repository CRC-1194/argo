
set terminal png  size 640,480 
set output "frequency.png"

D=0.001
n = 2.
sigma = 0.07275
rhol = 1000.
rhog = 1.
r0 = D/2.
dyn_viscl=1e-06*rhol
dyn_viscg=1.48e-05*rhog
omega0 = sqrt(sigma/((rhol+rhog)*r0**3))
omega2 = sqrt((n**3-n)*sigma/((rhol+rhog)*r0**3))
pi= 3.141
epsilon=0.05*r0
b=dyn_viscl/dyn_viscg


set xrange [0:0.03]


plot epsilon*exp(-b*x)*cos(omega2*x) ,\
    "postProcessing/interfaceHeight1/0/height.dat" u 1:(($2-D)/2)
