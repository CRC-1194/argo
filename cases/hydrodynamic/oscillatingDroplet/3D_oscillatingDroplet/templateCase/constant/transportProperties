FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (dispersed continuous);

dispersed
{
    transportModel  Newtonian;
    nu              1.0e-6;
    rho             1000.0; 
}

continuous
{
    transportModel  Newtonian;
    nu              1.48e-5;
    rho             1.0;
}

sigma               0.07275;

