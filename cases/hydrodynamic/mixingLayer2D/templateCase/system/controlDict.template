FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     interIsoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         @!endTime!@; //1e-3;

deltaT          @!timeStep!@; //1e-7;

writeControl    adjustable;

writeInterval   @!writeInterval!@; //1e-5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           @!CFL_num!@; //0.05;

maxAlphaCo      @!CFL_num!@;

maxDeltaT       1;


// ************************************************************************* //
